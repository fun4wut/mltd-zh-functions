{
  "bindings": [
    {
      "authLevel": "admin",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "events/{evtId:int}/rankings",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outputBlob",
      "path": "mltd-img/{DateTime}.png",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "../dist/manual_rank/index.js"
}
